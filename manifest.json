{
    "manifest_version": 3,
    "name": "TabIt",
    "version": "1.0.2",
    "description": "Save and organize open tabs into groups.",
    "permissions": ["tabs", "storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "assets/icons/tabit-icon.png"
    },
    "background": {
        "service_worker": "src/background.js"
    }
}
